{
  "name": "vite-plugin-svg-spritesheet",
  "version": "0.2.8",
  "description": "Vite plugin for SVG spritesheets with SVGO",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "vite build && changeset publish"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "spritesheet",
    "svg",
    "svgo"
  ],
  "author": "Jason Miiller <contact@jasonmiller.nl>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@microsoft/api-extractor": "^7.52.8",
    "@types/node": "^22.15.30",
    "@types/picomatch": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.5.3",
    "typescript": "^5.8.3",
    "unplugin-dts": "^1.0.0-beta.0",
    "vitest": "^3.2.3"
  },
  "peerDependencies": {
    "vite": "^6.3.5"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.4",
    "picomatch": "^4.0.2",
    "svgo": "^3.3.2",
    "tinyglobby": "^0.2.14"
  }
}
